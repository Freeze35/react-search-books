{
  "compilerOptions": {
    "baseUrl": ".",
    "compileOnSave": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "exclude": [
      "./src/tests/",

    ],
    "esModuleInterop": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "esnext",
    "target": "es5",
    "moduleResolution": "node"
  }
}
